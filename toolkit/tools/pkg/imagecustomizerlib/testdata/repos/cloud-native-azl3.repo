[cloud-native]
name=cloud-native
baseurl=https://packages.microsoft.com/azurelinux/3.0/prod/cloud-native/$basearch/
gpgcheck=1
repo_gpgcheck=1
enabled=1
# tdnf doesn't properly handle $basearch or gpgkey.
# But the key is the same for all archs.
gpgkey=https://packages.microsoft.com/azurelinux/3.0/prod/cloud-native/x86_64/repodata/repomd.xml.key
